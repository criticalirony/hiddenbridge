{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Test Web Server",
            "type": "python",
            "request": "launch",
            "module": "testws",
            "cwd": "${workspaceFolder}/python/testwebserver"
        },
        {
            "name": "Python: Generate plugins list",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/python/genpluginimport.py",
            "cwd": "${workspaceFolder}/python/",
            "console": "internalConsole",
            "args": [
                "../config.yml",
                "--output",
                "../plugin_config.go"
            ]
        },
        {
            "name": "Go: Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-config",
                "config.yml",
                "-v",
                "debug"
            ],
        }
    ]
}